package com.huawei;

import java.util.Collection;
import java.util.Iterator;
import java.util.Map;

public class T {

	public static void main(String[] args) {
		System.read("C://Users/管理员/Desktop/car.txt", "C://Users/管理员/Desktop/road.txt",
				"C://Users/管理员/Desktop/cross.txt");

		java.lang.System.out.println("---------------车----------");
		for (Car car : System.getCars().values()) {
			java.lang.System.out.println(car.toString());
		}
		java.lang.System.out.println("-------------路口--------------");
		for (Cross cross : System.getCrosses().values()) {
			java.lang.System.out.println(cross.getCrossID());
		}
		java.lang.System.out.println("------------路-----------");
		for (Road road : System.getRoads().values()) {
			java.lang.System.out.println(road.getId());
		}
//		System.launch();
